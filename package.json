{
    "name": "@sentry/n8n-nodes-sentry",
    "version": "0.1.0",
    "description": "n8n node for sending structured log messages to Sentry",
    "license": "MIT",
    "homepage": "https://github.com/getsentry/n8n-nodes-sentry",
    "keywords": [
        "n8n-community-node-package"
    ],
    "author": {
        "name": "Daniel Griesser",
        "email": "daniel.griesser.86@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/getsentry/n8n-nodes-sentry.git"
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
	"build": "npm run bundle && n8n-node build && npm run copy-bundle",
	"bundle": "vite build",
	"copy-bundle": "cp nodes/Sentry/sentry-sdk.bundle.* dist/nodes/Sentry/",
	"build:watch": "npm run bundle && tsc --watch",
	"dev": "n8n-node dev",
	"lint": "n8n-node lint",
	"lint:fix": "n8n-node lint --fix",
	"release": "n8n-node release",
	"prepublishOnly": "n8n-node prerelease"
},
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "strict": false,
        "credentials": [],
        "nodes": [
            "dist/nodes/Sentry/Sentry.node.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "@sentry/node": "^10.21.0",
        "eslint": "9.32.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2",
        "vite": "^7.0.0"
    },
    "dependencies": {},
    "peerDependencies": {
        "n8n-workflow": "*"
    }
}
